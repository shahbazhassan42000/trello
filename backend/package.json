    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.8.0",
    "mongoose": "^6.4.4",
    "mongoose-unique-validator": "^3.1.0",
    "morgan-body": "^2.3.1",
    "passport": "^0.6.0",
